{
  "version": 3,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "server.js",
      "methods": ["GET"]
    }
  ],
  "rewrites": [
    {
      "source": "/webhooks/(.*)",
      "destination": "server.js"
    }
  ]
}